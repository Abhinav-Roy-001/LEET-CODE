class Solution {
public:
    int numPairsDivisibleBy60(vector<int>& time) {
        vector<int> count(60, 0);
        int result = 0;
        
        for (int t : time) {
            int rem = t % 60;
            int partner = (60 - rem) % 60;
            result += count[partner];
            count[rem]++;
        }
        
        return result;
    }
};



OPTIMIZED APROACH
